{
  "name": "cma-dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "verify-env": "node scripts/verify-env.js",
    "prebuild": "npm run type-check && npm run verify-env",
    "auto-commit:start": "./scripts/auto-commit.sh",
    "auto-commit:stop": "pkill -f auto-commit.sh || true",
    "auto-commit:status": "ps aux | grep '[a]uto-commit.sh' || echo 'Auto-commit is not running'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
